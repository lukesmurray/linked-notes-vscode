{
  "name": "linked-notes-vscode",
  "displayName": "Linked Notes VSCode",
  "description": "Linked Note Taking and Wiki",
  "version": "0.0.13",
  "publisher": "lukesmurray",
  "repository": {
    "url": "https://github.com/lukesmurray/linked-notes-vscode.git",
    "type": "git"
  },
  "engines": {
    "vscode": "^1.44.0"
  },
  "keywords": [
    "markdown",
    "wiki",
    "hypertext"
  ],
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "markdown.markdownItPlugins": true,
    "languages": [
      {
        "id": "markdown-notes",
        "aliases": [],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "markdown-notes",
        "scopeName": "text.markdown.notes",
        "path": "./syntaxes/notes.tmLanguage.json",
        "injectTo": [
          "text.html.markdown"
        ]
      }
    ]
  },
  "icon": "images/linked-notes.png",
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "test-compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/lodash": "^4.14.150",
    "@types/markdown-it": "^10.0.1",
    "@types/mdast": "^3.0.3",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.1",
    "@types/redux-logger": "^3.0.7",
    "@types/vscode": "^1.44.0",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.1",
    "patch-package": "^6.2.2",
    "ts-loader": "^7.0.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.3.5",
    "citation-js": "^0.5.0-alpha.5",
    "citeproc": "^2.3.22",
    "latex-utensils": "^1.2.3",
    "lodash": "^4.17.15",
    "markdown-it-regex": "^0.2.0",
    "mdast-util-to-string": "^1.1.0",
    "redux-logger": "^3.0.6",
    "remark-parse": "^8.0.2",
    "remark-wiki-link": "0.0.4",
    "reselect-map": "^1.0.5",
    "unified": "^9.0.0",
    "unist-util-select": "^3.0.1",
    "unist-util-visit-parents": "^3.0.2"
  }
}
