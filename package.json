{
  "name": "linked-notes-vscode",
  "displayName": "Linked Notes VSCode",
  "description": "Linked Note Taking and Wiki",
  "version": "0.0.24",
  "publisher": "lukesmurray",
  "repository": {
    "url": "https://github.com/lukesmurray/linked-notes-vscode.git",
    "type": "git"
  },
  "engines": {
    "vscode": "^1.44.0"
  },
  "keywords": [
    "markdown",
    "wiki",
    "hypertext"
  ],
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "markdown.markdownItPlugins": true,
    "languages": [
      {
        "id": "linked-notes",
        "aliases": [],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "linked-notes",
        "scopeName": "text.markdown.notes",
        "path": "./syntaxes/notes.tmLanguage.json",
        "injectTo": [
          "text.html.markdown"
        ]
      }
    ],
    "commands": [
      {
        "command": "linked-notes-vscode.defaultWorkspaceConfig",
        "title": "Linked Notes: Write Default Workspace Config"
      },
      {
        "command": "linked-notes-vscode.newNote",
        "title": "Linked Notes: Create New Note"
      }
    ],
    "configuration": [
      {
        "title": "Linked Notes",
        "properties": {
          "linked-notes-vscode.defaultBib": {
            "type": [
              "string",
              "null"
            ],
            "default": null,
            "description": "Specifies the file path to the default CSL JSON used for citations. Relative to the workspace root."
          },
          "linked-notes-vscode.defaultReferencesFolder": {
            "type": [
              "string",
              "null"
            ],
            "default": "references",
            "description": "Specifies the file path to the default file created for citation reference files. Relative to the workspace root."
          }
        }
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "linked-notes-vscode.backlinksExplorerView",
          "name": "Backlinks"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "linked-notes-vscode.backlinksExplorerView",
        "contents": "No Backlinks have been found."
      }
    ]
  },
  "icon": "images/linked-notes.png",
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "scripts": {
    "analyze": "source-map-explorer 'dist/*.js'",
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "update-schemas": "./scripts/updateSchemas.sh",
    "compile": "tsc -p ./ --incremental",
    "postinstall": "patch-package",
    "lint": "eslint src --ext ts",
    "lint-fix": "eslint src --ext ts --fix-dry-run",
    "pretest": "npm run compile && npm run lint",
    "test-unit": "npm run compile && npm run lint && nyc mocha --ui='tdd' ./out/**/*.test.js",
    "test-integration": "npm run compile && npm run lint && node ./out/test/runTest.js",
    "test": "nyc mocha --ui='tdd' ./out/**.test.js && node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.2",
    "@types/lodash": "^4.14.150",
    "@types/markdown-it": "^10.0.1",
    "@types/mdast": "^3.0.3",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.1",
    "@types/vscode": "^1.44.0",
    "@types/xmldom": "^0.1.29",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard-with-typescript": "^18.0.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.10.0",
    "glob": "^7.1.6",
    "husky": "^4.2.5",
    "json-schema-to-typescript": "^9.1.0",
    "lint-staged": "^10.2.11",
    "mocha": "^7.2.0",
    "nyc": "^15.1.0",
    "patch-package": "^6.2.2",
    "prettier": "^2.0.5",
    "source-map-explorer": "^2.4.2",
    "ts-loader": "^7.0.2",
    "typescript": "^3.9.5",
    "vscode-test": "^1.3.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.3.5",
    "abortcontroller-polyfill": "^1.4.0",
    "ahocorasick": "^1.0.2",
    "citeproc": "^2.3.22",
    "lodash": "^4.17.15",
    "markdown-it-regex": "^0.2.0",
    "mdast-util-to-string": "^1.1.0",
    "memoize-one": "^5.1.1",
    "remark-parse": "^8.0.2",
    "reselect-map": "^1.0.5",
    "to-vfile": "^6.1.0",
    "unified": "^9.0.0",
    "unist-util-select": "^3.0.1",
    "unist-util-visit-parents": "^3.0.2",
    "vfile": "^4.1.1",
    "vfile-reporter": "^6.0.1",
    "xmldom": "^0.3.0"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "text"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  }
}
